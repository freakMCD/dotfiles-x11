
! Setting transparency and background                                                                                                                                       

!*.foreground: #BCBCBC
*background: [90]#0b090a  
! special
*.cursorColor: #afbfbf

Xft.dpi: 96
!! URxvt Appearance

!Font
URxvt.font:xft:JetBrainsMono Nerd Font:pixelsize=14
URxvt.boldFont:xft:JetBrainsMono Nerd Font:pixelsize=14


Xft.hintstyle:	hintslight
Xft.hinting: 1
Xft.antialias:1

!Font Spacing
URxvt.letterSpace: 0
URxvt.lineSpace: 0

!General Settings
URxvt*saveline: 2048
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*urgentOnBell: true
URxvt*iso14755: false

!Appearance
URxvt*depth:      32                                              
URxvt*transparency: true
!URxvt*tintColor: Blue
URxvt*shading : 18

! black
*color0: #1d1f21
*color8: #969698

! red
