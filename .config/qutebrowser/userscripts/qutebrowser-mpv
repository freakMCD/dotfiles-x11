#!/bin/sh
case $QUTE_URL in
    *twitch.tv/[!videos]*)
		#streamlink --twitch-api-header=Client-Id=ue6666qo983tsx6so1t0vnawi233wa --twitch-ttvlol --title '{author} - {title} - {category}' $QUTE_URL --player-args '--volume=80 --loop-playlist=inf' >/dev/null 2>&1 &exit;;  
		streamlink --twitch-ttvlol --twitch-proxy-playlist=https://eu.luminous.dev,https://as.luminous.dev --title '{author} - {title} - {category}' $QUTE_URL >/dev/null 2>&1 &exit;;  
	*)
		mpv $QUTE_URL >/dev/null 2>&1 &exit;;
esac
