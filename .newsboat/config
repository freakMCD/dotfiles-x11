browser qutebrowser
cleanup-on-quit no
notify-always yes
notify-program notify-send
notify-format "%d new articles (%n unread articles, %f unread feeds)"

macro M set browser "streamlink --player-passthrough=hls %u"; open-in-browser ; set browser qutebrowser




